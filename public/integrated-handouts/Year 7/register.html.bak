    <script>
        // Role-based form behavior
        document.getElementById('role').addEventListener('change', (e) => {
            const role = e.target.value;
            const yearLevelGroup = document.getElementById('yearLevelGroup');
            const roleDescription = document.getElementById('role-description');
            const yearLevelInput = document.getElementById('yearLevel');
            
            if (role === 'student') {
                yearLevelGroup.classList.add('show');
                yearLevelInput.required = true;
                roleDescription.textContent = 'Students can submit projects, collaborate with peers, and track their learning progress.';
            } else if (role === 'teacher') {
                yearLevelGroup.classList.remove('show');
                yearLevelInput.required = false;
                yearLevelInput.value = '';
                roleDescription.textContent = 'Teachers can create assignments, review student work, and access detailed analytics.';
            } else {
                yearLevelGroup.classList.remove('show');
                yearLevelInput.required = false;
                roleDescription.textContent = '';
            }
        });
        
        // Registration form handling
        document.getElementById('register-form').addEventListener('submit', async (e) => {
            e.preventDefault();
            
            const formData = new FormData(e.target);
            const data = {
                email: formData.get('email'),
                password: formData.get('password'),
                role: formData.get('role'),
                displayName: formData.get('displayName'),
                schoolName: formData.get('schoolName'),
                yearLevel: formData.get('yearLevel')
            };
            
            const messageDiv = document.getElementById('register-message');
            const registerButton = document.getElementById('register-button');
            const registerSpinner = document.getElementById('register-spinner');
            const registerText = document.getElementById('register-text');
            
            // Show loading state
            registerButton.disabled = true;
            registerSpinner.style.display = 'inline-block';
            registerText.textContent = 'Creating account...';
            messageDiv.style.display = 'none';
            
            try {
                const response = await fetch('/.netlify/functions/auth-register', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(data)
                });
                
                const result = await response.json();
                
                if (result.success) {
                    messageDiv.textContent = 'Registration successful! You can now sign in with your new account.';
                    messageDiv.className = 'message success';
                    messageDiv.style.display = 'block';
                    
                    // Clear form and redirect to login
                    document.getElementById('register-form').reset();
                    setTimeout(() => {
                        window.location.href = 'login.html';
                    }, 2000);
                    
                } else {
                    messageDiv.textContent = result.message || 'Registration failed. Please try again.';
                    messageDiv.className = 'message error';
                    messageDiv.style.display = 'block';
                }
            } catch (error) {
                console.error('Registration error:', error);
                messageDiv.textContent = 'An error occurred during registration. Please try again.';
                messageDiv.className = 'message error';
                messageDiv.style.display = 'block';
            } finally {
                // Reset loading state
                registerButton.disabled = false;
                registerSpinner.style.display = 'none';
                registerText.textContent = 'Create Account';
            }
        });
        
        // Check if user is already logged in
        window.addEventListener('load', () => {
            const token = localStorage.getItem('teKeteAko_token');
            if (token) {
                // Redirect to appropriate dashboard if already logged in
                const user = JSON.parse(localStorage.getItem('teKeteAko_user') || '{}');
                if (user.role === 'teacher') {
                    window.location.href = 'teacher-dashboard.html';
                } else {
                    window.location.href = 'student-dashboard.html';
                }
            }
        });
    </script>                    <footer class="site-footer no-print">
