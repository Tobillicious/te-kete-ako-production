    <script src="../js/tukutuku-pattern-generator.js"></script>
    <script>
        // Initialize the pattern generator when page loads
        let patternGenerator;

        document.addEventListener('DOMContentLoaded', function () {
            // Initialize the pattern generator
            patternGenerator = new TukutukuPatternGenerator('patternCanvas', 'controlsPanel');

            // Add some educational enhancements
            addEducationalFeatures();

            // Track user interactions for learning analytics
            trackLearningProgress();
        });

        function addEducationalFeatures() {
            // Add keyboard shortcuts for common actions
            document.addEventListener('keydown', function (e) {
                if (e.target.tagName.toLowerCase() === 'input') return;

                switch (e.key) {
                    case ' ': // Spacebar to toggle animation
                        e.preventDefault();
                        if (patternGenerator.animationId) {
                            patternGenerator.stopAnimation();
                        } else {
                            patternGenerator.startAnimation();
                        }
                        break;
                    case 'r': // R to reset
                        patternGenerator.resetView();
                        break;
                    case 's': // S to save
                        patternGenerator.savePattern();
                        break;
                }
            });

            // Add tooltips for mathematical terms
            addMathematicalTooltips();

            // Add progress tracking
            addProgressTracking();
        }

        function addMathematicalTooltips() {
            const tooltips = {
                'translation': 'Moving every point the same distance in the same direction',
                'rotation': 'Turning around a fixed center point',
                'reflection': 'Flipping across a mirror line',
                'scaling': 'Changing size while keeping proportions'
            };

            Object.keys(tooltips).forEach(term => {
                const elements = document.querySelectorAll(`[data-term="${term}"]`);
                elements.forEach(el => {
                    el.title = tooltips[term];
                    el.style.cursor = 'help';
                });
            });
        }

        function addProgressTracking() {
            const progress = {
                patternsExplored: new Set(),
                transformationsUsed: new Set(),
                animationsWatched: 0,
                patternsSaved: 0
            };

            // Track pattern changes
            const originalUpdateCulturalInfo = patternGenerator.updateCulturalInfo;
            patternGenerator.updateCulturalInfo = function () {
                originalUpdateCulturalInfo.call(this);
                progress.patternsExplored.add(this.settings.basePattern);
                progress.transformationsUsed.add(this.settings.transformation);
                updateProgressDisplay();
            };

            // Track animations
            const originalStartAnimation = patternGenerator.startAnimation;
            patternGenerator.startAnimation = function () {
                originalStartAnimation.call(this);
                progress.animationsWatched++;
                updateProgressDisplay();
            };

            // Track saves
            const originalSavePattern = patternGenerator.savePattern;
            patternGenerator.savePattern = function () {
                originalSavePattern.call(this);
                progress.patternsSaved++;
                updateProgressDisplay();
            };

            function updateProgressDisplay() {
                const progressDiv = document.querySelector('.progress-indicator');
                if (progressDiv) {
                    const completionPercentage = Math.round(
                        ((progress.patternsExplored.size / 4) * 25) +
                        ((progress.transformationsUsed.size / 4) * 25) +
                        (Math.min(progress.animationsWatched / 5, 1) * 25) +
                        (Math.min(progress.patternsSaved / 3, 1) * 25)
                    );

                    progressDiv.innerHTML = `
                        <h4>ğŸ† Learning Progress: ${completionPercentage}%</h4>
                        <div style="background: #ddd; border-radius: 10px; height: 20px; margin: 0.5rem 0;">
                            <div style="background: var(--color-accent); height: 100%; width: ${completionPercentage}%; border-radius: 10px; transition: width 0.3s ease;"></div>
                        </div>
                        <p>Patterns explored: ${progress.patternsExplored.size}/4 | Transformations used: ${progress.transformationsUsed.size}/4 | Animations watched: ${progress.animationsWatched} | Patterns saved: ${progress.patternsSaved}</p>
                    `;
                }
            }

            updateProgressDisplay();
        }

        function trackLearningProgress() {
            // Simple analytics to understand how students use the tool
            const analytics = {
                sessionStart: new Date(),
                interactions: 0,
                timeSpent: 0
            };

            // Track interaction frequency
            document.addEventListener('click', () => analytics.interactions++);
            document.addEventListener('change', () => analytics.interactions++);

            // Update time spent every minute
            setInterval(() => {
                analytics.timeSpent = Math.round((new Date() - analytics.sessionStart) / 60000);
            }, 60000);

            // Log session summary when leaving
            window.addEventListener('beforeunload', () => {
                console.log('Learning Session Summary:', analytics);
                // In a real implementation, this could send data to learning analytics system
            });
        }

        function toggleFullscreen() {
            const canvas = document.getElementById('patternCanvas');

            if (!document.fullscreenElement) {
                canvas.requestFullscreen().then(() => {
                    canvas.style.width = '100vw';
                    canvas.style.height = '100vh';
                    canvas.style.objectFit = 'contain';
                });
            } else {
                document.exitFullscreen().then(() => {
                    canvas.style.width = '';
                    canvas.style.height = '';
                    canvas.style.objectFit = '';
                });
            }
        }

        // Add educational context when patterns change
        function showPatternContext(patternName) {
            const contexts = {
                'diamond': 'PÄtikitiki patterns were often used to represent abundance and prosperity in traditional wharenui.',
                'triangle': 'Niho Taniwha patterns symbolized strength and protection, inspired by the teeth of the legendary taniwha.',
                'cross': 'Tawhiri patterns represented the four cardinal directions and the balance of natural forces.',
                'chevron': 'Roimata Toroa patterns symbolized peace and tranquility, like the gentle tears of the albatross.'
            };

            if (contexts[patternName]) {
                patternGenerator.showMessage(contexts[patternName], 'info');
            }
        }
    </script>
    

