        const supabase = window.supabase.createClient(SUPABASE_URL, SUPABASE_KEY);
        
        async function loadGraphRAGMathematics() {
            try {
                console.log('🧮 Loading Mathematics from GraphRAG Knowledge Graph...');
                
                // Query GraphRAG for mathematics resources
                const { data: mathData, error, count } = await supabase
                    .from('graphrag_resources')
                    .select('*', { count: 'exact' })
                    .or('subject.ilike.%math%,subject.ilike.%algebra%,subject.ilike.%geometry%,subject.ilike.%calculus%')
                    .order('quality_score', { ascending: false })
                    .limit(100);
                
                if (error) {
                    console.error('GraphRAG Error:', error);
                    return;
                }
                
                const totalCount = count || mathData.length;
                const culturalCount = mathData.filter(r => r.cultural_context).length;
                const teReoCount = mathData.filter(r => r.has_te_reo).length;
                const avgQuality = mathData.reduce((sum, r) => sum + (r.quality_score || 0), 0) / mathData.length;
                
                // Update UI
                const countEl = document.getElementById('math-resource-count');
                const culturalEl = document.getElementById('math-cultural-count');
                const teReoEl = document.getElementById('math-te-reo-count');
                const qualityEl = document.getElementById('math-quality-avg');
                
                if (countEl) countEl.textContent = totalCount.toLocaleString();
                if (culturalEl) culturalEl.textContent = culturalCount.toLocaleString();
                if (teReoEl) teReoEl.textContent = teReoCount.toLocaleString();
                if (qualityEl) qualityEl.textContent = avgQuality.toFixed(1);
                
                // Console success
