 <script>
 function calculateScore() {
 const checkboxes = document.querySelectorAll('.checkbox-container input');
 let checkedCount = 0;
 
 checkboxes.forEach(checkbox => {
 if (checkbox.checked) {
 checkedCount++;
 }
 });
 
 const score = (checkedCount / checkboxes.length) * 100;
 const resultElement = document.getElementById('score-result');
 
 let message = '';
 if (score >= 75) {
 message = `Your digital equity score is ${score}% - Excellent! Your classroom demonstrates strong digital equity practices.`;
 } else if (score >= 50) {
 message = `Your digital equity score is ${score}% - Good foundation with room for improvement.`;
 } else {
 message = `Your digital equity score is ${score}% - Consider focusing on digital equity strategies to support all learners.`;
 }
 
 resultElement.textContent = message;
 resultElement.style.fontWeight = 'bold';
 resultElement.style.marginTop = '15px';
 }
 </script>
