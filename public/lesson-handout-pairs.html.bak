<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lesson-Handout Pairs | Te Kete Ako</title>
    <meta name="description" content="1,100 linked lesson-handout pairs from knowledge graph">
<!-- Professional Design System -->
<link rel="stylesheet" href="/css/te-kete-professional.css">    
    <!-- CSS -->
    <!-- üé® ULTIMATE BEAUTY SYSTEM - Te Kete Ako Design Excellence -->
 <link rel="stylesheet" href="/css/te-kete-ultimate-beauty-system.css">
 <script src="https://cdn.tailwindcss.com"></script>
 <script src="/tailwind.config.ultimate.js"></script>
 <!-- END ULTIMATE BEAUTY SYSTEM --></head>
<body class="pattern-koru-subtle">
    <!-- Navigation -->
    <div id="nav-container"></div>
    <script>
        fetch('/components/navigation-standard.html')
            .then(r => r.text())
            .then(html => document.getElementById('nav-container').innerHTML = html);
    </script>
    
    <!-- Hero -->
    <section class="pairs-hero">
        <h1 style="font-size: 4rem; margin-bottom: 1rem; font-weight: 800;">üìù Lesson-Handout Pairs</h1>
        <p style="font-size: 1.5rem; opacity: 0.95;">
            1,100 perfectly matched lesson and handout combinations
        </p>
    </section>
    
    <main style="max-width: 1200px; margin: 3rem auto; padding: 0 2rem;">
        
        <div id="pairs-container">
            <div style="text-align: center; padding: 4rem; color: #888;">
                <p>Loading lesson-handout pairs...</p>
            </div>
        </div>
        
    </main>
    
    <!-- Footer -->
    <div id="footer-container"></div>
    <script>
        fetch('/components/footer.html').then(r=>r.text()).then(html=>{
            document.getElementById('footer-container').innerHTML=html;
        });
    </script>
    
    <script>
        // Load lesson-handout relationships from GraphRAG
        async function loadLessonHandoutPairs() {
            try {
                const url = 'https://nlgldaqtubrlcqddppbq.supabase.co/rest/v1/graphrag_relationships';
                const headers = {
                    'apikey': 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im5sZ2xkYXF0dWJybGNxZGRwcGJxIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTMwODkzMzksImV4cCI6MjA2ODY2NTMzOX0.IFaWqep1MBSofARiCUuzvAReC44hwGnmKOMNSd55nIM',
                    'Authorization': 'Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im5sZ2xkYXF0dWJybGNxZGRwcGJxIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTMwODkzMzksImV4cCI6MjA2ODY2NTMzOX0.IFaWqep1MBSofARiCUuzvAReC44hwGnmKOMNSd55nIM'
                };
                
                // Query for lesson-handout relationships
                const response = await fetch(`${url}?or=(relationship_type.eq.has_handout,relationship_type.eq.lesson_has_handout)&order=confidence.desc&limit=30`, { headers });
                const pairs = await response.json();
                
                displayPairs(pairs);
                
            } catch (error) {
                console.error('Error loading pairs:', error);
                document.getElementById('pairs-container').innerHTML = '<p style="color: red; text-align: center;">Error loading pairs. Please refresh.</p>';
            }
        }
        
        function displayPairs(pairs) {
            const container = document.getElementById('pairs-container');
            
            if (!pairs || pairs.length === 0) {
                container.innerHTML = '<p style="text-align: center;">No lesson-handout pairs found.</p>';
                return;
            }
            
            const html = pairs.map(pair => `
                <div class="pair-card">
                    <div class="lesson-side">
                        <div style="color: #0369a1; font-weight: 700; margin-bottom: 0.5rem;">üìö LESSON</div>
                        <div style="color: #1a4d2e; font-weight: 600; margin-bottom: 0.5rem;">
                            ${pair.source_path.split('/').pop().replace('.html', '').replace(/-/g, ' ')}
                        </div>
                        <a href="${pair.source_path}" style="color: #0284c7; text-decoration: none; font-size: 0.875rem;">
                            View lesson ‚Üí
                        </a>
                    </div>
                    
                    <div class="connection-arrow">
                        ‚Üí
                    </div>
                    
                    <div class="handout-side">
                        <div style="color: #ca8a04; font-weight: 700; margin-bottom: 0.5rem;">üìÑ HANDOUT</div>
                        <div style="color: #1a4d2e; font-weight: 600; margin-bottom: 0.5rem;">
                            ${pair.target_path.split('/').pop().replace('.html', '').replace(/-/g, ' ')}
                        </div>
                        <a href="${pair.target_path}" style="color: #f59e0b; text-decoration: none; font-size: 0.875rem;">
                            View handout ‚Üí
                        </a>
                    </div>
                </div>
            `).join('');
            
            container.innerHTML = html;
        }
        
        document.addEventListener('DOMContentLoaded', loadLessonHandoutPairs);
    </script>

<!-- üìä ULTIMATE BEAUTY: PostHog Analytics (Privacy-First) -->
<script src="/js/posthog-analytics.js" defer></script>

</body>
</html>

