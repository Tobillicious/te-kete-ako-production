 if (e.key === 'Enter') {
 const query = this.value.trim();
 if (query) {
 // Redirect to search page or implement search functionality
 window.location.href = `search.html?q=${encodeURIComponent(query)}`;
 }
 }
 });

 // Track 404 errors for analytics
